package com.example.newsService.core.repositories;

import com.example.newsService.core.comment.entities.CommentEntity;

import java.util.List;
import java.util.UUID;

public interface CommentRepository {
    void addComment(CommentEntity comment);
    CommentEntity getComment(UUID id);
    void updateComment(CommentEntity comment);
    void deleteComment(UUID id);
    List<CommentEntity> getAllComments();
}
